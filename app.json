{
  "expo": {
    "name": "oasis",
    "slug": "oasis",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Icono_apple.png",
        "backgroundColor": "#ffffff"
      },
      "notification": {
        "icon": "./assets/images/Icono_apple.png",
        "color": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-font"
    ]
  }
}